        <attr name="numColumns" format="integer"/>
        <attr name="numRows" format="integer"/>
        <attr name="dividerSize" format="dimension"/>
        <attr name="verticalDividerSize" format="dimension"/>
        <attr name="horizontalDividerSize" format="dimension"/>
        <attr name="horizontalDivider" format="color|reference"/>
        <attr name="verticalDivider" format="color|reference"/>
        <attr name="drawEndDivider" format="boolean"/>
        <attr name="indicatorOn" format="reference"/>
        <attr name="indicatorOff" format="reference"/>
        <attr name="indicatorHeight" format="dimension"/>
        <attr name="indicatorPadding" format="dimension"/>
        <attr name="paddingTop" format="dimension"/>
        <attr name="paddingLeft" format="dimension"/>
        <attr name="paddingRight" format="dimension"/>
    </declare-styleable>
</resources>